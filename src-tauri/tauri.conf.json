{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "com.divedock.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "windows": [
    {
      "title": "divedock",
      "width": 800,
      "height": 600
    }
  ],
  "allowlist": {
    "media": {
      "all": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
